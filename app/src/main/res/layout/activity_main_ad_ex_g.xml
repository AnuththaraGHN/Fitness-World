<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityAdExG">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarAdExG"
        android:layout_width="match_parent"
        android:layout_height="96dp"
        android:background="#FA2D73"
        android:contentDescription="@string/toolbarAdExF"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme" />


    <ImageView
        android:id="@+id/imgViewAdExG"
        android:layout_width="68dp"
        android:layout_height="54dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="337dp"
        android:layout_marginBottom="579dp"
        android:contentDescription="@string/imgViewAdExF"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />


    <TextView
        android:id="@+id/txtViewAdExG"
        android:layout_width="345dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-12dp"
        android:layout_marginBottom="582dp"
        android:contentDescription="@string/txtViewAdExF"
        android:text="20 min Advanced Workout Schedule No Equipment"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/txtViewJackKnife"
        android:layout_width="309dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="92dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="493dp"
        android:contentDescription="@string/txtViewSupermanHold"
        android:text="(F) Jack Knife"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/txtTimer13"
        android:layout_width="327dp"
        android:layout_height="158dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="46dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="40dp"
        android:contentDescription="@string/txtTimer12"
        android:drawableTop="@drawable/ic_baseline_timer_24"
        android:gravity="center"
        android:text="TextView"
        android:textColor="#971DAC"
        android:textSize="26sp"
        android:textStyle="bold" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifadg"
        android:layout_width="wrap_content"
        android:layout_height="326dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="200dp"
        android:contentDescription="@string/gifade"
        android:src="@drawable/exadg" />

</RelativeLayout>